{
  "name": "sb",
  "version": "0.0.0",
  "description": "Single-page-application boilerplate. Express server serving a React client via Webpack bundler.",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.jsx; exit 0",
    "webpack": "cd src/webpack && webpack serve --mode development --config config.js",
    "express": "NODE_ENV=development nodemon --watch 'src/express/*' --watch 'src/shared/*' src/express/index.js",
    "dev": "concurrently \"npm run express\" \"npm run webpack\"",
    "build": "cd src/webpack && webpack --mode production --config config.js",
    "start": "NODE_ENV=production node src/express/index.js",
    "app": "npm install && npm run build && npm run start",
    "pm2-start": "npm install && npm run build && pm2 start ecosystem.config.js && pm2 logs",
    "pm2-restart": "npm install && npm run build && pm2 restart ecosystem.config.js --update-env && pm2 logs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nameer-rizvi/sb.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "webpack",
    "babel",
    "single page application",
    "spa"
  ],
  "author": "Nameer Rizvi (https://github.com/nameer-rizvi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nameer-rizvi/sb/issues"
  },
  "homepage": "https://github.com/nameer-rizvi/sb#readme",
